strong {
  padding: 0 1px;
  color: #f22f27;
  font-weight: 600px;
  font-size: 1em;
}
blockquote {
  border-radius: 4px;
  border-left: 4px solid #42b983;
  padding: 10px 15px;
  color: #606266;
  background-color: rgba(66, 185, 131, .1);
}
body {
  cursor: url('/Hand.cur'), auto;
}
::selection {
  background:#A6FFFF;
  color:#606266;
}

::-moz-selection {
  background:#A6FFFF;
  color:#606266;
}

::-webkit-selection {
  background:#A6FFFF;
  color:#606266;
}